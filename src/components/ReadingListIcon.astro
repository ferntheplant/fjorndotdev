---
import { FileText, BookOpen, Youtube, Code } from "@lucide/astro";

export interface Props {
  type: "article" | "book" | "youtube" | "code";
}

const { type } = Astro.props;

const iconMap = {
  article: FileText,
  book: BookOpen,
  youtube: Youtube,
  code: Code,
};

const IconComponent = iconMap[type];
---

<IconComponent class="stroke-primary" />
